package aprimoramentoJava;

import java.util.Scanner;

public class AccountTeste {

	public static void main(String[] args) {
		
		
		Scanner input = new Scanner(System.in);
		
	
		Account minhaConta = new Account();
	
		System.out.println(minhaConta.getName());
		
		System.out.println("Por favor entre com o nome:");
		String theNome = input.nextLine();
		minhaConta.setName(theNome);
		System.out.println();
		
		System.out.println("Nome do obejto conta é: " + minhaConta.getName());
		
		

	}

}
