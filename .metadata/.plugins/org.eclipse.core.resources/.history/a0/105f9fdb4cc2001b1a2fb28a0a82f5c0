package aula03FolhaDePagamento;

public class Principal {

	public static void main(String[] args) {
		Funcionario zezinho = new Funcionario();
		zezinho.nome= "Zezinho";
		zezinho.cargo= "Analista de TI";
		double salario = zezinho.salario.calcularSalario(160, 12, 35.5, 40.2);
		
		System.out.println("Salário calculado: " + salario);
	}

}
